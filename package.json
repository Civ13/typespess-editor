{
	"name": "typespess-editor",
	"main": "./lib/typespess-editor",
	"version": "0.0.1",
	"description": "A short description of your package",
	"keywords": [],
	"repository": "https://github.com/taislin/typespess-editor",
	"license": "MIT",
	"engines": {
		"atom": ">=1.0.0 <2.0.0"
	},
	"dependencies": {
		"cson": "^7.20.0",
		"fs-plus": "^3.1.1",
		"json-stable-stringify": "^1.0.1"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"es2020": true,
			"node": true
		},
		"globals": {
			"atom": false
		},
		"extends": [
			"eslint:recommended"
		],
		"parserOptions": {
			"ecmaVersion": 11
		},
		"rules": {
			"@typescript-eslint/no-var-requires": 0,
			"@typescript-eslint/no-explicit-any": 0
		}
		
	},
	"consumedServices": {
		"status-bar": {
			"versions": {
				"^1.0.0": "consumeStatusBar"
			}
		}
	}
