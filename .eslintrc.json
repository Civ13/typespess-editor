{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2020": true,
		"node": true
	},
	"globals": {
		"atom": false
	},
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "@typescript-eslint/no-var-requires": 0,
        "@typescript-eslint/no-explicit-any": 0
    }
}
